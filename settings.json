        "exclude": [
            "markdown",
            "json",
            "matlab"
        ],
    },
    "psi-header.lang-config": [
        {
            "language": "python",
            "begin": "###############################################################################",
            "prefix": "# ",
            "suffix": "#",
            "end": "###############################################################################",
            "blankLinesAfter": 2,
            "lineLength": 79,
        },
        {
            "language": "matlab",
            "begin": "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "prefix": "% ",
            "end": "%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
            "blankLinesAfter": 2,
            "lineLength": 80,
        },
    ],
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "File Created: <<filecreated('Do MMMM YYYY')>>",
                "Author 1: <<author>> (<<authoremail>>)",
                "-----",
                "Last Modified: <<dateformat('Do MMMM YYYY')>>",
                "Modified By: <<author>> (<<authoremail>>)",
                "-----",
                "Company: <<company>>",
                "-----",
                "Description:"
            ]
        },
        {
            "language": "python",
